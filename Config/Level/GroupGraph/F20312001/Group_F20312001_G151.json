{
  "OnInitSequece": [
    {
      "TaskList": []
    }
  ],
  "OnStartSequece": [
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.LoopWaitServerFloorSavedValueChange"
        }
      ]
    },
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.PredicateTaskList",
          "Predicate": {
            "$type": "RPG.GameCore.ByCompareFloorSavedValue",
            "Name": "ToyRoomState",
            "CompareType": "Equal"
          },
          "SuccessTaskList": [
            {
              "$type": "RPG.GameCore.TriggerEntityServerEvent"
            }
          ],
          "FailedTaskList": [
            {
              "$type": "RPG.GameCore.PredicateTaskList",
              "Predicate": {
                "$type": "RPG.GameCore.ByCompareFloorSavedValue",
                "Name": "ToyRoomState",
                "CompareType": "Equal",
                "CompareValue": 1
              },
              "SuccessTaskList": [
                {
                  "$type": "RPG.GameCore.TriggerEntityServerEvent"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "Type": "Group"
}