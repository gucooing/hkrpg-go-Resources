            }
          ],
          "TargetType": {
            "$type": "RPG.GameCore.TargetFetchAdvPropEx",
            "FetchType": "SinglePropByPropID",
            "SinglePropID": {
              "GroupID": {
                "IsDynamic": false,
                "FixedValue": {
                  "Value": 3
                }
              },
              "ID": {
                "IsDynamic": false,
                "FixedValue": {
                  "Value": 300009
                }
              }
            }
          }
        }
