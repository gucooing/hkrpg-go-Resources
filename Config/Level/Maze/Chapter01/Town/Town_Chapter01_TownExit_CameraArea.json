{
  "OnInitSequece": [],
  "OnStartSequece": [
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.PropSetupTrigger",
          "TargetType": {
            "$type": "RPG.GameCore.TargetFetchAdvPropEx"
          },
          "TargetGroupID": {
            "IsDynamic": false,
            "FixedValue": {
              "Value": 0
            }
          },
          "TargetID": {
            "IsDynamic": false,
            "FixedValue": {
              "Value": 0
            }
          },
          "TargetTypes": [],
          "OnTriggerEnter": [
            {
              "$type": "RPG.GameCore.PredicateTaskList",
              "Predicate": {
                "$type": "RPG.GameCore.ByAnchorAngleDiffWithMainCameraOverThan",
                "LevelAreaCameraKey": {
                  "Custom": true,
                  "Key": "Anchor"
                },
                "Angle": 90
              },
              "SuccessTaskList": [
                {
                  "$type": "RPG.GameCore.ActiveVirtualCamera",
                  "LevelAreaCameraKey": {
                    "Custom": true,
                    "Key": "Camera2"
                  },
                  "IsEntryPointCamera": true,
                  "LookAtTargetAnchorName": "LocalPlayer",
                  "BlendConfig": {
                    "ExportToJson": true,
                    "BlendType": "EaseInOut",
                    "BlendTime": 1
                  }
                }
              ],
              "FailedTaskList": [
                {
                  "$type": "RPG.GameCore.ActiveVirtualCamera",
                  "LevelAreaCameraKey": {
                    "Custom": true,
                    "Key": "Camera1"
                  },
                  "IsEntryPointCamera": true,
                  "LookAtTargetAnchorName": "LocalPlayer",
                  "BlendConfig": {
                    "ExportToJson": true,
                    "BlendType": "EaseInOut",
                    "BlendTime": 1
                  }
                }
              ]
            }
          ],
          "OnTriggerExit": [
            {
              "$type": "RPG.GameCore.ActiveVirtualCamera",
              "LevelAreaCameraKey": {
                "Custom": true,
                "Key": "Camera1"
              },
              "IsEntryPointCamera": true,
              "IsActive": false,
              "BlendConfig": {
                "ExportToJson": true,
                "BlendType": "Custom",
                "CustomCurveName": "TownExitCommonCameraExitCurve",
                "BlendTime": 1
              }
            },
            {
              "$type": "RPG.GameCore.ActiveVirtualCamera",
              "LevelAreaCameraKey": {
                "Custom": true,
                "Key": "Camera2"
              },
              "IsEntryPointCamera": true,
              "IsActive": false,
              "BlendConfig": {
                "ExportToJson": true,
                "BlendType": "Custom",
                "CustomCurveName": "TownExitCommonCameraExitCurve",
                "BlendTime": 1
              }
            }
          ]
        }
      ]
    }
  ],
  "ValueSource": {
    "Values": [
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "Camera1"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "Camera2"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "Anchor"
      }
    ]
  }
}