{
  "OnInitSequece": [],
  "OnStartSequece": [
    {
      "IsLoop": true,
      "TaskList": [
        {
          "$type": "RPG.GameCore.PuzzleBoxmanInBoard",
          "OnPlayerEnter": [
            {
              "$type": "RPG.GameCore.ActiveVirtualCamera",
              "AreaName": "WaterLevel_Camera",
              "AnchorName": "C1",
              "LevelAreaCameraKey": {
                "Custom": true,
                "Key": "BoxMan_Cam"
              },
              "BlendConfig": {
                "ExportToJson": true,
                "BlendType": "Linear",
                "BlendTime": 1
              }
            }
          ],
          "OnPlayerLeave": [
            {
              "$type": "RPG.GameCore.ActiveVirtualCamera",
              "AreaName": "WaterLevel_Camera",
              "AnchorName": "C1",
              "LevelAreaCameraKey": {
                "Custom": true,
                "Key": "BoxMan_Cam"
              },
              "IsActive": false,
              "BlendConfig": {
                "ExportToJson": true,
                "BlendType": "Cut",
                "BlendTime": 1
              }
            }
          ]
        }
      ]
    },
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.ShowWaypointByProp",
          "GroupID": {
            "IsDynamic": false,
            "FixedValue": {
              "Value": 5
            }
          },
          "InstanceID": {
            "IsDynamic": false,
            "FixedValue": {
              "Value": 300005
            }
          },
          "MaxRange": 300,
          "IconPath": "SpriteOutput/MapPics/Billboard/IconBillboardMainMission02.png",
          "Offset": {
            "X": 0,
            "Y": 1.5,
            "Z": 0
          }
        }
      ]
    },
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.PropInitPuzzle",
          "BoardPath": {
            "Custom": true,
            "Key": "BoxMan_BoardPath"
          },
          "LocalPosition": {
            "X": 0,
            "Y": 0,
            "Z": 0
          }
        }
      ]
    },
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.PropPuzzleEventListener",
          "FinishCallback": [
            {
              "$type": "RPG.GameCore.PropClosePuzzle"
            },
            {
              "$type": "RPG.GameCore.ActiveVirtualCamera",
              "AreaName": "WaterLevel_Camera",
              "AnchorName": "C1",
              "LevelAreaCameraKey": {
                "Custom": true,
                "Key": "BoxMan_Cam"
              },
              "IsActive": false,
              "BlendConfig": {
                "ExportToJson": true,
                "BlendType": "Cut"
              }
            }
          ],
          "ResetCallback": []
        }
      ]
    },
    {
      "TaskList": []
    },
    {
      "IsLoop": true,
      "TaskList": [
        {
          "$type": "RPG.GameCore.WaitCustomString",
          "CustomString": {
            "Custom": true,
            "Key": "BoxMan_LevelClose"
          }
        }
      ]
    },
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.PropStateChangeListenerConfig",
          "FromState": "Open",
          "ToState": "Locked",
          "OnChange": [
            {
              "$type": "RPG.GameCore.PropMoveTo",
              "Duration": {
                "IsDynamic": false,
                "FixedValue": {
                  "Value": 1.5
                }
              },
              "LevelAreaKey": {
                "Custom": true,
                "Key": "High"
              },
              "TargetType": {
                "$type": "RPG.GameCore.TargetFetchAdvPropEx"
              }
            }
          ],
          "TargetType": {
            "$type": "RPG.GameCore.TargetFetchAdvPropEx",
            "FetchType": "SinglePropByPropKey",
            "SinglePropKey": {
              "Custom": true,
              "Key": "BindWater"
            }
          }
        }
      ]
    },
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.PropStateChangeListenerConfig",
          "FromState": "Locked",
          "ToState": "Open",
          "OnChange": [
            {
              "$type": "RPG.GameCore.PropMoveTo",
              "Duration": {
                "IsDynamic": false,
                "FixedValue": {
                  "Value": 1.5
                }
              },
              "LevelAreaKey": {
                "Custom": true,
                "Key": "Low"
              },
              "TargetType": {
                "$type": "RPG.GameCore.TargetFetchAdvPropEx"
              }
            }
          ],
          "TargetType": {
            "$type": "RPG.GameCore.TargetFetchAdvPropEx",
            "FetchType": "SinglePropByPropKey",
            "SinglePropKey": {
              "Custom": true,
              "Key": "BindWater"
            }
          }
        }
      ]
    }
  ],
  "ValueSource": {
    "Values": [
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "BoxMan_Cam"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "BoxMan_LevelStart"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "BoxMan_LevelClose"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "BoxMan_BoardPath"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "BindWater"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "High"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "Low"
      }
    ]
  }
}