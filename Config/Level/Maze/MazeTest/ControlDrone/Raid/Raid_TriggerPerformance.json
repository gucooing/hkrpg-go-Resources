  "ValueSource": {
    "Values": [
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "TriggerPerformance"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "Mission_Str"
      }
    ]
  }
