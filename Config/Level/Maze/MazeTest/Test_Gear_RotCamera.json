{
  "OnInitSequece": [],
  "OnStartSequece": [
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.PropSetupTrigger",
          "TargetType": {
            "$type": "RPG.GameCore.TargetFetchAdvPropEx"
          },
          "TargetGroupID": {
            "IsDynamic": false,
            "FixedValue": {
              "Value": 0
            }
          },
          "TargetID": {
            "IsDynamic": false,
            "FixedValue": {
              "Value": 0
            }
          },
          "TargetTypes": [
            "LocalPlayer"
          ],
          "OnTriggerEnter": [
            {
              "$type": "RPG.GameCore.ActiveVirtualCamera",
              "LevelAreaCameraKey": {
                "Custom": true,
                "Key": "CameraName"
              },
              "IsEntryPointCamera": true,
              "BlendConfig": {
                "ExportToJson": true,
                "BlendType": "EaseInOut",
                "BlendTime": 2
              }
            }
          ],
          "OnTriggerExit": [
            {
              "$type": "RPG.GameCore.ActiveVirtualCamera",
              "LevelAreaCameraKey": {
                "Custom": true,
                "Key": "CameraName"
              },
              "IsEntryPointCamera": true,
              "IsActive": false,
              "BlendConfig": {
                "ExportToJson": true,
                "BlendType": "EaseInOut",
                "BlendTime": 2
              }
            }
          ]
        }
      ]
    }
  ],
  "ValueSource": {
    "Values": [
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "CameraName"
      }
    ]
  }
}